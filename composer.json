            }
        }
    },
    "require": {
        "composer/installers": "^1.0",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "^8.3.0",
        "drupal/jsonapi": "^1.0",
        "drupal/simple_oauth": "^2.0",
        "drupal/jsonapi_extras": "^1.0",
