{
    "name": "contentacms/contenta_jsonapi",
    "description": "",
    "type": "drupal-profile",
    "authors": [
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0+",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "4.2.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/enyo/dropzone",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }

    ],
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/core": {
                "Allow a profile to be installed from existing config": "https://www.drupal.org/files/issues/2788777-33.patch"
            }
        }
    },
    "require": {
        "composer/installers": "^1.0",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "^8.3.0",
        "drupal/jsonapi": "^1.0",
        "drupal/simple_oauth": "^2.0",
        "drupal/recipes_magazin": "1.x-dev",
        "drupal/jsonapi_extras": "^1.0",
        "drupal/docson": "^1.0",
        "drupal/subrequests": "^1.0",
        "drupal/fieldable_path": "^1.0",
        "drupal/restui": "^1.14",
        "roave/security-advisories": "dev-master",
        "drupal/media_entity": "^1.6",
        "drupal/entity_browser": "^1.0",
        "drupal/media_entity_image": "^1.2",
        "drupal/dropzonejs": "^1.0@alpha",
        "enyo/dropzone": "4.2.0"
    },
    "scripts": {
        "install-contenta": "scripts/development/build-contenta_jsonapi.sh"
    },
    "provide": {
    },
    "require-dev": {
        "burdamagazinorg/thunder-dev-tools": "dev-master",
        "drupal/config_inspector": "1.x-dev",
        "drupal/coder": "^8.2",
        "drupal/features": "^3.5"
    }
}
